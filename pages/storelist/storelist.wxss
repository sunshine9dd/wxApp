.store-list {
}

.store-item {
  width: 100%;
  height: 98rpx;
  padding-left: 30rpx;
}

.store-item>view {
  width: 100%;
  height: 98rpx;
  border-bottom: 1rpx solid #E8E8E8;
  font-size: 30rpx;
  color: #000;
}

.store-item:active {
  background-color: #F5F5F5;
}

.scroll-info{
  width: 100%;
  height: calc(100vh - 60rpx);
}